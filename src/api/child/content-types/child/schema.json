{
  "kind": "collectionType",
  "collectionName": "children",
  "info": {
    "singularName": "child",
    "pluralName": "children",
    "displayName": "Sous expertises"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre_google": {
      "type": "string",
      "required": true
    },
    "description_google": {
      "type": "text",
      "required": true
    },
    "structured_google": {
      "type": "json",
      "required": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "titre": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "titre",
      "required": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "expertises.mode",
        "expertises.left",
        "expertises.items",
        "expertises.fournisseurs",
        "expertises.avantages",
        "expertises.items-mode",
        "offre.offre",
        "expertises.types",
        "expertises.prime"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "expertise": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::expertise.expertise",
      "inversedBy": "children"
    }
  }
}
